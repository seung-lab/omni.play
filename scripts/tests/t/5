# The test data
%OMNITEST = (
	'cmd' => '
create:{$OUTDIR}/{$TESTNAME}.omni
loadHDF5seg:{$DATADIR}5.h5
loadHDF5chann:{$DATADIR}5.h5
split:1,2
quit
		'
		,
	'finish' => '$SUCCESS = -e "{$OUTDIR}/{$TESTNAME}.omni.files" && sane();'
		,
	'markup' => '$MARKUP = start_html("Test: {$TESTNAME}"). 
					h1("Test: {$TESTNAME} ".result()).
						"<a href=\"{$OUTDIR}/{$TESTNAME}.omni.files/\">resultant omni</a>".
			       end_html();'
);

